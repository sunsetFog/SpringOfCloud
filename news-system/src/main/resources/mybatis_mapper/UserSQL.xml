<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stars.mapper.news.UserMapper">
    <select id="loginVerify" resultType="com.stars.pojo.news.LoginParams" parameterType="Map">
        SELECT * FROM ums_admin where username = #{username, jdbcType=VARCHAR} and password = #{password, jdbcType=VARCHAR};
    </select>
</mapper>